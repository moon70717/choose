<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="api">
	<insert id="apidata" parameterType="map">
		insert ignore apidata(code, placename,mapx,mapy,tel,img,addr,modifiedtime)
		values(#{contentid},#{title},#{mapy},#{mapx},#{tel},#{firstimage},#{addr1},#{modifiedtime})	
	</insert>
</mapper>  